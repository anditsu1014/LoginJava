<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee"  id="WebApp_ID" version="3.1">
  <servlet>
      <servlet-name>InicioSesion</servlet-name>
      <servlet-class>Conrolador.InicioSesion</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>RegistrarUsuarios</servlet-name>
      <servlet-class>Conrolador.RegistrarUsuarios</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>InicioSesion</servlet-name>
      <url-pattern>/login</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
      <servlet-name>RegistrarUsuarios</servlet-name>
      <url-pattern>/nuevoUsuario</url-pattern>
   </servlet-mapping>
</web-app>